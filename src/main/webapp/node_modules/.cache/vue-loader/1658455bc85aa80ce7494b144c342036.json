        type: [{required: true, message: '请选择字典类型', trigger: 'blur'}],\n            },\n            submitLoading: false,\n            delLoading: false,\n            dictId: '',\n            dictName: ''\n        }\n    },\n    created() {\n        this.$nextTick(() => {\n            this.query();\n        })\n    },\n    methods: {\n        query() {\n            this.page();\n        },\n        pageInit() {\n            this.pageUrl = '/back/dict/page'\n            return true;\n        },\n        subDelete(id) {\n            this.delLoading = true\n            delDict(id).then(res => {\n                this.delLoading = false\n                this.$refs[id].doClose()\n                this.$notify({\n                    title: '删除成功',\n                    type: 'success',\n                    duration: 2500\n                });\n                this.page();\n            }).catch(err => {\n                this.delLoading = false\n                this.$refs[id].doClose()\n                console.log(err.response.data.message)\n            })\n        },\n        handleCurrentChange(val) {\n            if (val) {\n                this.$refs.dictData.dictName = val.name\n                this.$refs.dictData.dictId = val.id\n                this.$refs.dictData.init()\n            }\n        },\n        doEdit(row) {\n            this.dialog = true;\n            this.isAdd = false;\n            this.form = this.copyObj(row);\n        },\n        doAdd() {\n            this.isAdd = true;\n            this.dialog = true;\n        },\n        cancel() {\n            this.resetForm();\n        },\n        doSubmit() {\n\n            this.submitLoading = true;\n            this.$refs['form'].validate((valid) => {\n                if (valid) {\n                    if (this.isAdd) {\n                        //新增\n                        saveDict(this.form).then(res => {\n                            this.$message({\n                                message: '新增成功!',\n                                type: 'success'\n                            });\n                            this.submitLoading = false;\n                            this.cancel();\n                            this.query();\n                        }).catch(err => {\n                            this.submitLoading = false;\n                        })\n                    } else {\n                        //编辑\n                        editDict(this.form).then(res => {\n                            this.$message({\n                                message: '编辑成功!',\n                                type: 'success'\n                            });\n                            this.submitLoading = false;\n                            this.cancel();\n                            this.query();\n                        }).catch(err => {\n                            this.submitLoading = false;\n                        })\n                    }\n                } else {\n                    this.submitLoading = false;\n                }\n            })\n\n        },\n        resetForm() {\n            this.dialog = false;\n            this.form = {};\n            this.$refs['form'].resetFields();\n        }\n\n    }\n}\n",{"version":3,"sources":["Index.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4IA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA","file":"Index.vue","sourceRoot":"src/views/model/support/dict","sourcesContent":["<template>\n    <div class=\"app-container\">\n        <el-row :gutter=\"10\">\n            <el-col :xs=\"24\" :sm=\"24\" :md=\"10\" :lg=\"10\" :xl=\"10\">\n                <el-card class=\"box-card\">\n                    <div slot=\"header\" class=\"clearfix\">\n                        <span>字典列表</span>\n                        <el-button\n                                class=\"filter-item\"\n                                size=\"mini\"\n                                style=\"float: right;padding: 4px 10px\"\n                                type=\"primary\"\n                                icon=\"el-icon-plus\"\n                                @click=\"doAdd\">新增\n                        </el-button>\n\n                        <el-button\n                                size=\"mini\"\n                                type=\"success\"\n                                style=\"float: right;padding: 4px 10px\"\n                                icon=\"el-icon-search\"\n                                @click=\"query\">\n                            查询\n                        </el-button>\n                    </div>\n\n                    <div class=\"head-container\">\n                        <el-form :inline=\"true\">\n                            <el-form-item label=\"字典名\">\n                                <el-input\n                                        v-model=\"pageInfo.params.name\"\n                                        clearable\n                                        style=\"width: 100px\">\n                                </el-input>\n                            </el-form-item>\n\n                            <el-form-item label=\"字典编码\">\n                                <el-input\n                                        v-model=\"pageInfo.params.code\"\n                                        clearable\n                                        style=\"width: 100px\">\n                                </el-input>\n                            </el-form-item>\n\n                            <el-form-item label=\"类型\">\n                                <el-select\n                                        v-model=\"pageInfo.params.type\"\n                                        placeholder=\"请选择\"\n                                        clearable\n                                        style=\"width: 110px\">\n                                    <el-option value=\"0\" label=\"普通字典\"/>\n                                    <el-option value=\"1\" label=\"SQL字典\"/>\n                                </el-select>\n                            </el-form-item>\n\n                        </el-form>\n\n                    </div>\n\n                    <!--表格渲染-->\n                    <el-table v-loading=\"pageLoading\" :data=\"pageInfo.records\" size=\"small\" highlight-current-row\n                              style=\"width: 100%;\"\n                              @current-change=\"handleCurrentChange\">\n                        <el-table-column :show-overflow-tooltip=\"true\" prop=\"name\" label=\"名称\"/>\n                        <el-table-column :show-overflow-tooltip=\"true\" prop=\"code\" label=\"编码\"/>\n                        <el-table-column prop=\"type\" label=\"类型\">\n                            <template slot-scope=\"scope\">\n                                <span>{{ scope.row.type===0?'普通字典':'SQL字典' }}</span>\n                            </template>\n                        </el-table-column>\n                        <el-table-column :show-overflow-tooltip=\"true\" prop=\"description\" label=\"描述\"/>\n                        <el-table-column label=\"操作\" width=\"130px\" align=\"center\">\n                            <template slot-scope=\"scope\">\n                                <el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\"\n                                           @click=\"doEdit(scope.row)\"/>\n                                <el-popover\n                                        :ref=\"scope.row.id\"\n                                        placement=\"top\"\n                                        width=\"180\">\n                                    <p>此操作将删除字典与对应的字典数据，确定要删除吗？</p>\n                                    <div style=\"text-align: right; margin: 0\">\n                                        <el-button size=\"mini\" type=\"text\" @click=\"$refs[scope.row.id].doClose()\">取消\n                                        </el-button>\n                                        <el-button :loading=\"delLoading\" type=\"primary\" size=\"mini\"\n                                                   @click=\"subDelete(scope.row.id)\">确定\n                                        </el-button>\n                                    </div>\n                                    <el-button slot=\"reference\" type=\"danger\" icon=\"el-icon-delete\"\n                                               size=\"mini\"/>\n                                </el-popover>\n                            </template>\n                        </el-table-column>\n                    </el-table>\n                    <!--分页组件-->\n                    <el-pagination\n                            :total=\"pageInfo.total\"\n                            style=\"margin-top: 8px;\"\n                            layout=\"total, prev, pager, next, sizes\"\n                            @size-change=\"sizeChange\"\n                            @current-change=\"pageChange\">\n                    </el-pagination>\n                </el-card>\n            </el-col>\n            <el-col :xs=\"24\" :sm=\"24\" :md=\"14\" :lg=\"14\" :xl=\"14\">\n                <dict-data ref=\"dictData\"/>\n            </el-col>\n        </el-row>\n\n        <el-dialog :visible.sync=\"dialog\" :title=\"isAdd ? '新增字典' : '编辑字典'\" append-to-body width=\"550px\" @close=\"cancel\">\n            <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" size=\"small\" label-width=\"100px\">\n                <el-form-item label=\"字典名称\" prop=\"name\">\n                    <el-input v-model=\"form.name\" clearable/>\n                </el-form-item>\n                <el-form-item label=\"字典编码\" prop=\"code\">\n                    <el-input v-model=\"form.code\"/>\n                </el-form-item>\n                <el-form-item label=\"字典类型\" prop=\"type\">\n                    <el-select\n                            v-model=\"form.type\"\n                            placeholder=\"请选择\"\n                            clearable\n                            style=\"width: 110px\">\n                        <el-option :value=\"0\" label=\"普通字典\"/>\n                        <el-option :value=\"1\" label=\"SQL字典\"/>\n                    </el-select>\n                </el-form-item>\n                <el-form-item label=\"描述\" prop=\"description\">\n                    <el-input v-model=\"form.description\"/>\n                </el-form-item>\n\n            </el-form>\n            <div slot=\"footer\" class=\"dialog-footer\">\n                <el-button type=\"text\" @click=\"cancel\">取消</el-button>\n                <el-button :loading=\"submitLoading\" type=\"primary\" @click=\"doSubmit\">确认</el-button>\n            </div>\n        </el-dialog>\n    </div>\n</template>\n\n<script>\n    import {saveDict, editDict, delDict} from './Api.js'\n    import page from '@/mixins/page'\n\n    import dictData from './DictData'\n\n    export default {\n        components: {dictData},\n        mixins: [page],\n        data() {\n            return {\n                dialog: false,\n                isAdd: true,\n                form: {},\n                rules: {\n                    name: [{required: true, message: '请输入字典名称', trigger: 'blur'}],\n                    code: [{required: true, message: '请输入字典编码', trigger: 'blur'}],\n                    type: [{required: true, message: '请选择字典类型', trigger: 'blur'}],\n                },\n                submitLoading: false,\n                delLoading: false,\n                dictId: '',\n                dictName: ''\n            }\n        },\n        created() {\n            this.$nextTick(() => {\n                this.query();\n            })\n        },\n        methods: {\n            query() {\n                this.page();\n            },\n            pageInit() {\n                this.pageUrl = '/back/dict/page'\n                return true;\n            },\n            subDelete(id) {\n                this.delLoading = true\n                delDict(id).then(res => {\n                    this.delLoading = false\n                    this.$refs[id].doClose()\n                    this.$notify({\n                        title: '删除成功',\n                        type: 'success',\n                        duration: 2500\n                    });\n                    this.page();\n                }).catch(err => {\n                    this.delLoading = false\n                    this.$refs[id].doClose()\n                    console.log(err.response.data.message)\n                })\n            },\n            handleCurrentChange(val) {\n                if (val) {\n                    this.$refs.dictData.dictName = val.name\n                    this.$refs.dictData.dictId = val.id\n                    this.$refs.dictData.init()\n                }\n            },\n            doEdit(row) {\n                this.dialog = true;\n                this.isAdd = false;\n                this.form = this.copyObj(row);\n            },\n            doAdd() {\n                this.isAdd = true;\n                this.dialog = true;\n            },\n            cancel() {\n                this.resetForm();\n            },\n            doSubmit() {\n\n                this.submitLoading = true;\n                this.$refs['form'].validate((valid) => {\n                    if (valid) {\n                        if (this.isAdd) {\n                            //新增\n                            saveDict(this.form).then(res => {\n                                this.$message({\n                                    message: '新增成功!',\n                                    type: 'success'\n                                });\n                                this.submitLoading = false;\n                                this.cancel();\n                                this.query();\n                            }).catch(err => {\n                                this.submitLoading = false;\n                            })\n                        } else {\n                            //编辑\n                            editDict(this.form).then(res => {\n                                this.$message({\n                                    message: '编辑成功!',\n                                    type: 'success'\n                                });\n                                this.submitLoading = false;\n                                this.cancel();\n                                this.query();\n                            }).catch(err => {\n                                this.submitLoading = false;\n                            })\n                        }\n                    } else {\n                        this.submitLoading = false;\n                    }\n                })\n\n            },\n            resetForm() {\n                this.dialog = false;\n                this.form = {};\n                this.$refs['form'].resetFields();\n            }\n\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>\n"]}]}